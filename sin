double a, b, eps, k, x;
x = double.Parse(Console.ReadLine());
a = Math.Sin(x);
eps = double.Parse(Console.ReadLine());
k = 0;
b = 0;
while (b < eps)
{
    b = a;
    a = ((double)Math.Sin((Math.Sin(a))));
    k++;
    Console.WriteLine("-");
}
Console.WriteLine(b);
Console.WriteLine(k);
Console.WriteLine(a);
